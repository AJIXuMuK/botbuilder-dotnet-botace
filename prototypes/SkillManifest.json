{
  "@context": "https://botframework.com/schemas",
  "@type": "skill",
  "@version": "1.0",
  "id": "TodoSkill",
  "msaAppId": "e7401b5c-c097-4f0c-9d8d-0e94ad9acdab",
  "endpoints": [
    {
      "type": "BotFramework V3",
      "endpoint": "https://example.com/api/messages"
    }
  ],
  "iconUrl": "./icons.png",
  "events": {
    "createTodo": {
      "value": {
        "type": "object",
        "properties": {
          "title": { "type": "string" }
        }
      },
      "returnValue": { "$ref": "#/definitions/todo" }
    }
  },
  "definitions": {
    "todo": {
      "type": "object",
      "properties": {
        "title": { "type": "string" },
        "created": { "type": "string" }
      },
      "required": [ "title", "created" ]
    }
  },
  "files": [
    "./todoSkill.en.lu",
    "./todoSkill.es.lu"
  ]
}