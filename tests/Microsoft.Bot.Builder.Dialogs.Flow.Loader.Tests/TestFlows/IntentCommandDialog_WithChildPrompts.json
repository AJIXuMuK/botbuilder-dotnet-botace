{
  "@type": "http://schemas.botframework.com/IntentCommandDialog",
  "Id": "intentDialog",
  "Recognizer": {
    "@type": "http://schemas.botframework.com/RuleRecognizer",
    "Rules": {
      "name": "name",
      "age": "age"
    }
  },
  "Commands": {
    "name": {
      "@type": "http://schemas.botframework.com/CallDialog",
      "DialogId": "NamePrompt"
    },
    "age": {
      "@type": "http://schemas.botframework.com/CallDialog",
      "DialogId": "NumberPrompt"
    },
    "None": {
      "@type": "http://schemas.botframework.com/SendActivity",
      "Text": "Sorry, I didn't get that"
    }
  },
  "Dialogs": [
    {
      "@type": "http://schemas.botframework.com/TextPrompt",
      "Id": "NamePrompt",
      "DefaultOptions": {
        "Prompt": {
          "type": "message",
          "text": "What is your name?"
        },
        "RetryPrompt": {
          "type": "message",
          "text": "Reprompt: What is your name?"
        }
      }
    },
    {
      "@type": "http://schemas.botframework.com/IntNumberPrompt",
      "Id": "NumberPrompt",
      "DefaultOptions": {
        "Prompt": {
          "type": "message",
          "text": "What is your age?"
        },
        "RetryPrompt": {
          "type": "message",
          "text": "Reprompt: What is your age?"
        }
      }
    }
  ]
}