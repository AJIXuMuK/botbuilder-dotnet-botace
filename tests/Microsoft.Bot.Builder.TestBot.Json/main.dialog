{
  "$schema": "./Schemas/app.schema",
  "$type": "Microsoft.SequenceDialog",
  "$id": "getUserInfo",
  "sequence": [
    {
      "$type": "http://schemas.botframework.com/CallDialog",
      "dialog": {
        "$type": "http://schemas.botframework.com/TextPrompt",
        "$id": "namePrompt",
        "property": "name",
        "pattern": "\\w{3,50}",
        "initialPrompt": "What's your name?",
        "retryPrompt": "Let's try again, what's your name?",
        "notMatchedActivity": "You need to give me at least 3 chars to 30 chars as a name."
      }
    },
    {
      "$type": "http://schemas.botframework.com/SendActivityTemplateStep",
      "activityTemplate": "{template(welcome-user)}"
    },
    {
      "$type": "http://schemas.botframework.com/CallDialog",
      "dialog": {
        "$type": "http://schemas.botframework.com/TextPrompt",
        "$id": "countryPrompt",
        "property": "country",
        "pattern": "\\w{3,50}",
        "initialPrompt": "{template(prompt-country)}",
        "retryPrompt": "Let's try again, what's your country of birth?",
        "notMatchedActivity": "You need to give me at least 3 chars to 30 chars as a country."
      }
    },
    {
      "$type": "http://schemas.botframework.com/SendActivityTemplateStep",
      "activityTemplate": {
        "template": "love-country"
      }
    },
    {
      "$type": "http://schemas.botframework.com/EndDialog",
      "result": "value"
    }
  ]
}