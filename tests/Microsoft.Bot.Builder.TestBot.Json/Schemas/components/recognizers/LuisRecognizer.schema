{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "LUIS Recognizer",
    "description": "LUIS recognizer.",
    "$implements": [
        "Microsoft.IRecognizer"
    ],
    "allOf": [
        {
            "$ref": "../component.schema#"
        },
        {
            "additionalProperties": false,
            "patternProperties": {
                "^$": {
                    "type": "string"
                }
            },
            "properties": {
                "$type": {
                    "const": "Microsoft.LUISRecognizer"
                },
                "$id": {
                    "type": "string"
                },
                "application": {
                    "type": "object",
                    "properties": {
                        "applicationId": {
                            "type": "string"
                        },
                        "endpoint": {
                            "type": "string"
                        },
                        "endpointKey": {
                            "type": "string"
                        }
                    },
                    "required": [
                        "applicationId",
                        "endpoint",
                        "endpointKey"
                    ]
                },
                "priority": {
                    "type": "string"
                }
            },
            "required": [
                "applicationId"
            ]
        }
    ]
}